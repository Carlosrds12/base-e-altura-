#include <stdio.h>

int main() {
    float base, altura, area, perimetro;

    printf("Digite a base do retangulo: ");
    scanf("%f", &base);

    printf("Digite a altura do retangulo: ");
    scanf("%f", &altura);

    area = base * altura;
    perimetro = 2 * (base + altura);

    printf("Area: %f\n", area);
    printf("Perimetro: %f\n", perimetro);

    return 0;
}
